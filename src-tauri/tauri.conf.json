{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Learning Board",
  "version": "0.1.1",
  "identifier": "com.learning.board",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Learning Board",
        "width": 1280,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "../public/frontend-version.json"
    ]
  },
  "plugins": {
    "updater": {
      "active": true,
      "dialog": true,
      "endpoints": [
        "https://raw.githubusercontent.com/Dapper-Dan777/Private-Dashboard/main/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEYwQjNBRTI1NEUwNjY0OTYKUldTV1pBWk9KYTZ6OE8yejlyQVpORUlkZ2RJb1U0K2J5dW9US3lzZE5aNVVyeG1XRVBSRmZHQ0wK"
    }
  }
}
